{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MyTauriAppTest",
  "version": "0.1.0",
  "identifier": "com.example.mytauriapp",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "My Framer app",
        "width": 800,
        "height": 600, 
        "resizable": true,
        "fullscreen": false,
        "alwaysOnTop": true,
        "url": "index.html",
        "visible": false,
        "transparent": true,
        "decorations": false 
      },
      {
        "label": "control",
        "title": "Framer Controls",
        "url": "control.html",
        "width": 350,
        "height": 50,
        "x": 2385,
        "y": 0,
        "resizable": false,
        "decorations": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["control-window-access"]
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
  }
}