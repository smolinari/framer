{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MyTauriAppTest",
  "version": "0.1.0",
  "identifier": "com.example.mytauriapp",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Framer", 
        "url": "index.html",
        "maximized": true,   
        "transparent": true, 
        "decorations": false,
        "alwaysOnTop": true, 
        "visible": true,     
        "resizable": false,  
        "fullscreen": false,
        "skipTaskbar": true  
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["control-window-access"]
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
  }
}